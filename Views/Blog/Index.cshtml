@{
    ViewData["Title"] = "Blog Page";
}
@{
    var data = ViewBag.List;
    int i = 1;

}



<div>
    <h1>This is my blog page</h1>
    <ul>
      @{ foreach (var d in data)
          {
              
        <li><a href="/Blog/@i">@d</a></li>
        i++;
              }
          }
    </ul>
    
</div>